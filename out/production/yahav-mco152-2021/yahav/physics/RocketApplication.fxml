<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.*?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Text?>
<GridPane fx:controller="yahav.physics.RocketController"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <padding>
        <Insets top="25" right="2" bottom="10" left="2"/>
    </padding>


    <Label  text="Velocity:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>

    <TextField fx:id="velocityField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

    <Label text="Angle:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>

    <TextField fx:id="angleField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

    <Label text="Seconds:" GridPane.columnIndex="0" GridPane.rowIndex="2"/>

    <TextField fx:id="secondsField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>

    <Button fx:id="calculateButton" text="Calculate" GridPane.columnIndex="1" GridPane.rowIndex="3"
            onAction="#calculateRocket"/>

    <Label text="Time to Land:" GridPane.columnIndex="0" GridPane.rowIndex="4"/>

    <TextField fx:id="timeToLand" GridPane.columnIndex="1" GridPane.rowIndex="4"/>

    <Label text="X:" GridPane.columnIndex="0" GridPane.rowIndex="5"/>

     <TextField fx:id="xField" GridPane.columnIndex="1" GridPane.rowIndex="5"/>

    <Label text="Y:" GridPane.columnIndex="0" GridPane.rowIndex="6"/>

    <TextField fx:id="yField" GridPane.columnIndex="1" GridPane.rowIndex="6"/>



</GridPane>
